<h2>Request Logs</h2>

<table>
  <thead>
  <th>Query</th>
  <th>Response</th>
  </thead>
  <tbody>
  <% log_entries.each do |entry| %>
    <tr>
      <td><%= entry.query %></td>
      <td><%= entry.response or 'No response' %></td>
      <td><%= entry.exact ? 'Exact' : 'Calculated' %></td>
    </tr>
  <% end %>
  </tbody>
</table>
